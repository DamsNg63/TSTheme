}

QTabBar QToolButton::right-arrow {													/* Tab Bar Scroll Button Right */
	image: url('styles:DarkenTS/gui/arrows/right.png');
}

QTabBar QToolButton::left-arrow:disabled {
	image: url('styles:DarkenTS/gui/arrows/left_disabled.png');
}

QTabBar QToolButton::right-arrow:disabled {
	image: url('styles:DarkenTS/gui/arrows/right_disabled.png');
}

