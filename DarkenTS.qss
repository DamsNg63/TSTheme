QDateEdit:disabled,
QDateTimeEdit:disabled {
	background-color: rgb(40,40,40);
	color: rgb(72,72,72);
}
	
QTimeEdit:focus,
QTimeEdit:focus:hover,
QDateEdit:focus,
QDateEdit:focus:hover,
QDateTimeEdit:focus,
QDateTimeEdit:focus:hover {
	background-color: rgb(62,79,94);
}

QTimeEdit:hover,
QDateEdit:hover,
QDateTimeEdit:hover {
	border: 0px solid transparent;
	background-color: rgb(85,85,85);
}
	
QTimeEdit::up-button,
QDateEdit::up-button,
QDateTimeEdit::up-button,
QTimeEdit::down-button,
QDateEdit::down-button,
QDateTimeEdit::down-button {
	border: 0px solid transparent;
}

QTimeEdit::up-arrow,
QDateEdit::up-arrow,
QDateTimeEdit::up-arrow {
	border: 0px solid transparent;
	image: url('styles:DarkenTS/gui/arrows/up.png');
		height: 10px;
		width: 10px;
}

QTimeEdit::down-arrow,
QDateEdit::down-arrow,
QDateTimeEdit::down-arrow {
	border: 0px solid transparent;
	image: url('styles:DarkenTS/gui/arrows/down.png');
		height: 10px;
		width: 10px;
}

QTimeEdit::up-arrow:disabled,
QDateEdit::up-arrow:disabled,
QDateTimeEdit::up-arrow:disabled {
	image: url('styles:DarkenTS/gui/arrows/up_disabled.png');
		height: 10px;
		width: 10px;
}

QTimeEdit::down-arrow:disabled,
QDateEdit::down-arrow:disabled,
QDateTimeEdit::down-arrow:disabled {
	image: url('styles:DarkenTS/gui/arrows/down_disabled.png');
		height: 10px;
		width: 10px;
}

QTimeEdit::drop-down,
QDateEdit::drop-down,
QDateTimeEdit::drop-down {
	border: 0px solid transparent;
	background-color: transparent;
	subcontrol-origin: padding;
		subcontrol-position: top right;
}

/* -- Calendar -- */

QCalendarWidget QWidget#qt_calendar_navigationbar {
	background-color: rgb(68,68,68);														/* Background NavBar */
}

QCalendarWidget QToolButton {
	width: 80px;
	height: 25px;
}

QCalendarWidget QToolButton::menu-indicator {
		subcontrol-position: center right;
}

QCalendarWidget QWidget {
	alternate-background-color: rgb(34,34,34);												/* Background Header */
}
   
QCalendarWidget QAbstractItemView:enabled  {
	background-color: rgb(46,46,46);														/* Background Calendar */
	selection-background-color: rgb(62,79,94);												/* Selected day */
	selection-color: rgb(255,255,255);
}

QCalendarWidget QAbstractItemView:disabled {												/* Non-Month Day */
	color: rgb(72,72,72);
}

